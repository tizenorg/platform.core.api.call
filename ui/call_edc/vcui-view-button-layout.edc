group{
	name: "separator-layout";
	parts{
		part { name: "background";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 153;
			}
		}
		
		part { name: "top_horiz_sep";
			type: RECT;
			description {
				state: "default" 0.0;
				min: 0 1;
				rel1 { relative: 0 0; offset: 0 -1; to: "background";}
				rel2 { relative: 1 0; to: "background";}
				color: 80 80 80 192;
			}
		}

		part { name: "topline_left_vert_1";
			type: RECT;
			scale: 1;
			description {
				state: "default" 0.0;
				min: 1 0;
