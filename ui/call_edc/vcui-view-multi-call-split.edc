			effect: SHADOW;
			scale: 1;
			description {
				state: "default" 0.0;
				visible: 0;
				text { font, FONT_SLP_Roman; size, 44; align, 0.5 0.5; }
				color: 255 255 255 255;
				color2: 0 0 0 255;
			}
			description {
				state: "end" 0.0;
				inherit: "default" 0.0;
				visible: 0;
			}
			description {
				state: "portrait" 0.0;
				inherit: "default" 0.0;
				visible: 1;
				rel1 { relative: MULTICALL_SPLIT_TIMER_L MULTICALL_SPLIT_TIMER_T; }
				rel2 { relative: MULTICALL_SPLIT_TIMER_R MULTICALL_SPLIT_TIMER_B; }				
			}
		}	

		part { name: "btn_bigend";
			type: SWALLOW;
			scale: 1;
			description {
				state: "default" 0.0;
				visible: 0;
			}
			description {
				state: "portrait" 0.0;
				visible: 1;
				rel1 { relative: SIX_BTN_BIG_END_L SIX_BTN_BIG_END_T; to: "background";}
