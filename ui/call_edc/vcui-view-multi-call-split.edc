				visible: 0;
			}
			description {
				state: "portrait" 0.0;
				visible: 1;
				rel1 { relative: MULTICALL_SPLIT_CID_1_L MULTICALL_SPLIT_CID_1_T; }
				rel2 { relative: MULTICALL_SPLIT_CID_1_R MULTICALL_SPLIT_CID_1_B; }
			}
		}
		
		part { name: "swl_pause";
			type: SWALLOW;
			description {
				state: "default" 0.0;
				visible: 0;
			}
			description {
				state: "portrait" 0.0;
				visible: 1;
